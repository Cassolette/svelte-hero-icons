{
  "name": "svelte-hero-icons",
  "version": "1.0.4",
  "description": "Heroicons for Svelte (Project based on heroicons)",
  "main": "index.js",
  "module": "index.mjs",
  "svelte": "src/index.js",
  "files": [
    "src",
    "index.mjs",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
