    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "dependencies": {}
