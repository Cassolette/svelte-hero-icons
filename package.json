{
  "name": "svelte-hero-icons",
  "version": "2.1.0",
  "description": "Heroicons for Svelte (Project based on heroicons)",
  "types": "types/index.d.ts",
  "main": "src/index.js",
  "svelte": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustinVoitel/svelte-hero-icons"
  },
  "files": [
    "src",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "",
    "build:icons": "ts-node build.ts",
    "build": "npm run clean && npm run build:icons",
    "clean": "rm -rf src/heroicons src/index.js types/index.d.ts ",
    "test": "ts-node test/index.ts",
    "prepublishOnly": "npm run build"
  },
  "author": "Justin Voitel <justin.voitel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.10",
    "heroicons": "^1.0.0",
    "html-parse-stringify": "^2.0.0",
    "pascalcase": "^1.0.0",
    "svelte": "^3.35.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}